{
  "name": "extension",
  "version": "2.2",
  "manifest_version": 2,
  "description": "test插件2.2",
  "author": "严素静",
  "icons": {
    "16": "img/cat2.png",
    "48": "img/cat2.png",
    "128": "img/cat2.png"
  },
  "browser_action": {
    "default_icon": "img/cat2.png",
    "default_title": "test插件",
    "default_popup": "index.html"
  },
  "background": {
    "page": "back.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "*://*/*",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/handle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "chrome_url_overrides": {}
}